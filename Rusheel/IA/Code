package CSVData;

import java.awt.EventQueue;
import java.awt.Font;

import javax.swing.JFrame;

import java.awt.CardLayout;

import javax.swing.JPanel;

import java.awt.Color;

import javax.swing.JLabel;
import javax.swing.JOptionPane;
import javax.swing.SwingConstants;
import javax.swing.JTextField;
import javax.swing.JButton;

import java.awt.event.ActionListener;
import java.awt.event.ActionEvent;
import javax.swing.JRadioButton;

public class Information {

	private JFrame frame;
	private JTextField textField;
	private JPanel panel;
	private JPanel panel_1;
	
	/**
	 * Launch the application.
	 */
	public static void Information() {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					Information window = new Information();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public Information() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		
		PlayerData data = new PlayerData();
		data.loadDataFromFile("/Users/RusheelIyer/Desktop/Data.csv");
		
		frame = new JFrame();
		frame.setBounds(100, 100, 450, 300);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.getContentPane().setLayout(new CardLayout(0, 0));
		
		final JPanel panel = new JPanel();
		panel.setBackground(Color.DARK_GRAY);
		frame.getContentPane().add(panel, "name_47856221983511");
		panel.setLayout(null);
		panel.setVisible(true);
		
		final JPanel panel_1 = new JPanel();
		panel_1.setBackground(Color.DARK_GRAY);
		frame.getContentPane().add(panel_1, "name_48070864161784");
		panel_1.setLayout(null);
		
		JLabel lblWhatWouldYou = new JLabel("What would you like to know?");
		lblWhatWouldYou.setHorizontalAlignment(SwingConstants.CENTER);
		lblWhatWouldYou.setForeground(Color.WHITE);
		lblWhatWouldYou.setFont(new Font("Avenir Next Condensed", Font.BOLD | Font.ITALIC, 14));
		lblWhatWouldYou.setBounds(88, 6, 260, 16);
		panel_1.add(lblWhatWouldYou);
		panel_1.setVisible(false);
		
		JLabel lblNewLabel = new JLabel("Enter the Name Of the Player You Would Like To Search For:");
		lblNewLabel.setHorizontalAlignment(SwingConstants.CENTER);
		lblNewLabel.setForeground(Color.WHITE);
		lblNewLabel.setFont(new Font("Avenir Next Condensed", Font.BOLD | Font.ITALIC, 14));
		lblNewLabel.setBounds(46, 68, 354, 16);
		panel.add(lblNewLabel);
		
		textField = new JTextField();
		textField.setBounds(56, 96, 344, 28);
		panel.add(textField);
		textField.setColumns(10);
		
		JButton btnSearch = new JButton("Search");
		btnSearch.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnSearch.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String userInput = textField.getText().toLowerCase();
				for(int i = data.size(); i >= 0; i--){
					
					if(data.getName(i).toLowerCase().contains(userInput)){
						continue;
					}else{
						data.remove(i);
					}
					
					
				}
				
				if((data.size()) == 0){
					String name = data.getName(0);
					JOptionPane.showConfirmDialog(null, "Are you looking for " + name);
					
					panel_1.setVisible(true);
					panel.setVisible(false);
				}else if(data.size() > 0){
					JOptionPane.showMessageDialog(null, "Please Enter the Full Name of the Player");
				}else{
					JOptionPane.showMessageDialog(null, "I could not find the player you are looking for");
				}
				
			}
		});
		btnSearch.setBounds(162, 191, 117, 29);
		panel.add(btnSearch);
		
		JButton btnBack_1 = new JButton("Back");
		btnBack_1.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnBack_1.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				FirstOption move = new FirstOption();
				move.FirstOption();
				
			}
		});
		btnBack_1.setBounds(6, 243, 117, 29);
		panel.add(btnBack_1);
		
		JButton btnAge = new JButton("Age");
		btnAge.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				int age = data.getAge(0);
				JOptionPane.showMessageDialog(null,name + " is " + age + " years old");
				
			}
		});
		btnAge.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnAge.setBounds(53, 54, 140, 29);
		panel_1.add(btnAge);
		
		JButton btnNationality = new JButton("Nation");
		btnNationality.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				String nation = data.getNationality(0);
				JOptionPane.showMessageDialog(null,name + " is from " + nation);
				
			}
		});
		btnNationality.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnNationality.setBounds(261, 54, 140, 29);
		panel_1.add(btnNationality);
		
		JButton btnPreferredFoot = new JButton("Preferred Foot");
		btnPreferredFoot.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				String preferredFoot = data.getFoot(0);
				if(preferredFoot.equalsIgnoreCase("l")){
					preferredFoot = "Left";
				}else{
					preferredFoot = "Right";
				}
				JOptionPane.showMessageDialog(null,name + " prefers his " + preferredFoot + " foot");
				
			}
		});
		btnPreferredFoot.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnPreferredFoot.setBounds(53, 104, 140, 29);
		panel_1.add(btnPreferredFoot);
		
		JButton btnClub = new JButton("Current Club");
		btnClub.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				String club = data.getClub(0);
				JOptionPane.showMessageDialog(null,name + " plays for " + club);
				
			}
		});
		btnClub.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnClub.setBounds(261, 104, 140, 29);
		panel_1.add(btnClub);
		
		JButton btnHeight = new JButton("Height");
		btnHeight.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				int height = data.getHeight(0);
				JOptionPane.showMessageDialog(null,name + " is " + height + " CentiMetres tall");
				
			}
		});
		btnHeight.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnHeight.setBounds(53, 154, 140, 29);
		panel_1.add(btnHeight);
		
		JButton btnPosition = new JButton("Position");
		btnPosition.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				String position = data.getPosition(0);
				JOptionPane.showMessageDialog(null,name + " plays as a " + position);
				
			}
		});
		btnPosition.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnPosition.setBounds(261, 154, 140, 29);
		panel_1.add(btnPosition);
		
		JButton btnBack = new JButton("Back");
		btnBack.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnBack.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				panel.setVisible(true);
				panel_1.setVisible(false);
				
			}
		});
		btnBack.setBounds(6, 243, 117, 29);
		panel_1.add(btnBack);
		
		JButton btnNewButton = new JButton("Kit Number");
		btnNewButton.setFont(new Font("Franklin Gothic Medium", Font.BOLD, 16));
		btnNewButton.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				String name = data.getName(0);
				int kitNumber = data.getKitNumber(0);
				JOptionPane.showMessageDialog(null,name + " wears the number " + kitNumber);
				
			}
		});
		btnNewButton.setBounds(165, 204, 140, 29);
		panel_1.add(btnNewButton);
	}
}

