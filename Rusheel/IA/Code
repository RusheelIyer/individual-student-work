package CSVData;

public class PlayerRecord {
	
	private int age, height, kitNumber;
	private String name, position, nationality, foot, clubName;
	
	public PlayerRecord(int age, int height, String name, String position,
			String nationality, String foot, String clubName, int kitNumber) {

		this.age = age;
		this.height = height;
		this.name = name;
		this.position = position;
		this.nationality = nationality;
		this.foot = foot;
		this.clubName = clubName;
		this.kitNumber = kitNumber;
		
	}
	
	public int getKitNumber() {
		return kitNumber;
	}

	public int getAge() {
		return age;
	}
	
	public int getHeight() {
		return height;
	}
	
	public String getName() {
		return name;
	}
	
	public String getPosition() {
		return position;
	}
	
	public String getNationality() {
		return nationality;
	}
	
	
	public String getFoot() {
		return foot;
	}
	
	public String getClubName() {
		return clubName;
	}
	
}

package CSVData;

import java.io.BufferedWriter;
import java.io.FileReader;
import java.io.FileWriter;
import java.io.IOException;
import java.io.PrintWriter;
import java.util.ArrayList;
import java.util.LinkedList;
import java.util.Scanner;

import javax.swing.JOptionPane;

public class PlayerData {

	private ArrayList<PlayerRecord> records;
	
	public PlayerData(){
		
		records = new ArrayList<PlayerRecord>();
		
	}
	
	void loadDataFromFile(String string) {
		try{
			
		Scanner read = new Scanner(new FileReader("/Users/RusheelIyer/Desktop/Data.csv"));	
		
		String line;
		PlayerRecord record;
		
		read.nextLine();
		while(read.hasNextLine()){
			line = read.nextLine();
			String[] results = line.split(",");
			
			String name = results[0];
			int age = Integer.parseInt(results[1]);
			String position = results[2];
			String nationality = results[3];
			String foot = results[4];
			String clubName = results[5];
			int height = Integer.parseInt(results[6]);
			int kitNumber = Integer.parseInt(results[7]);
			
			record = new PlayerRecord(age, height, name, position, nationality, foot, clubName, kitNumber);
			
			records.add(record);
		}
		read.close();
		}catch(Exception e){
			
			JOptionPane.showMessageDialog(null, e);
			
		}
		
	}

	public String getName(int i) {
		return records.get(i).getName();	
	}

	public void remove(int i) {
		records.remove(i);
	}

	public int size() {
		return (records.size())-1;
	}

	public String getFoot(int i) {
		return records.get(i).getFoot();
	}

	public int getAge(int i) {
		return records.get(i).getAge();
	}

	public String getNationality(int i) {
		return records.get(i).getNationality();
	}

	public String getPosition(int i) {
		return records.get(i).getPosition();
	}

	public String getClub(int i) {
		return records.get(i).getClubName();
	}

	public int getHeight(int i) {
		return records.get(i).getHeight();
	}

	public int getKitNumber(int i) {
		return records.get(i).getKitNumber();
	}
	
}

