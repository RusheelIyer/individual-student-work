				for(int i = data.size(); i >= 0; i--){
					
					if(data.getHeight(i) >= 180){
						continue;
					}else{
						data.remove(i);
					} 
				}check(data);
			}
			private void check(PlayerData data) {
				
				if(data.size() == 0){
					JOptionPane.showMessageDialog(null, "The player you are thinking of is: " + data.getName(0));
					JOptionPane.showMessageDialog(null, "I managed to guess it in 7 tries");
					
					panel_17.setVisible(true);
					panel_6.setVisible(false);
				}else{
					
					panel_7.setVisible(true);
					panel_6.setVisible(false);
					
					question8nationality.setText("Is your player from " + data.getNationality(0) + "?");
				}	
			}
		});
		button_4.setBounds(86, 128, 117, 29);
		panel_6.add(button_4);
		
