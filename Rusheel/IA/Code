		btnNo.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				for(int i = data.size(); i >= 0; i--){
					
					if(data.getFoot(i).equals("L")){
						data.remove(i);
					}else{
						continue;
					} 
						
				}	check(data);
			}
			private void check(PlayerData data) {
				
				if(data.size() == 0){
					JOptionPane.showMessageDialog(null, "The player you are thinking of is: " + data.getName(0));
					
					JOptionPane.showMessageDialog(null, "I managed to guess it in 1 try");
					panel_17.setVisible(true);
					panel.setVisible(false);
				}else{
					
					panel_1.setVisible(true);
					panel.setVisible(false);
					
				}
			}
		});
