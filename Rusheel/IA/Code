			public void actionPerformed(ActionEvent e) {
				
				try {
					
				File file = new File("/Users/RusheelIyer/Desktop/names.txt");
				FileWriter playerName = new FileWriter(file.getAbsoluteFile(), true);	
				playerName.write(textField.getText() + "\n");
				playerName.close();
				
				JOptionPane.showMessageDialog(null, "Added. We will consider adding this player. Thank You!");
				
				} catch (IOException e1) {
					e1.printStackTrace();
				}
				
				
			}

		});
		btnAdd.setBounds(169, 170, 117, 29);
		panel_17.add(btnAdd);
	}
}
